name: nyc_airbnb_dev
conda_env: conda.yml

entry_points:
  main:
    parameters:
      steps:
        description: Comma-separated list of steps to execute
        type: str
        default: all
      hydra_options:
        description: Other configuration parameters to override
        type: str
        default: ''
    command: "python main.py main.steps=\\'{steps}\\' $(echo {hydra_options})"
