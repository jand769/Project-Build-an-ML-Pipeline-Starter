name: nyc_airbnb
conda_env: conda.yml

entry_points:
  main:
    parameters:
      steps:
        type: str
        default: all
      hydra_options:
        type: str
        default: ''
    command: >
      export PYTHONPATH=$PYTHONPATH:/workspace/Project-Build-an-ML-Pipeline-Starter/components &&
      python main.py main.steps='{steps}' $(echo {hydra_options})
